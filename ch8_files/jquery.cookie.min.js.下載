// cookie相关方法

! function(e) {
	"function" == typeof define && define.amd ? define(["jquery"], e) : "object" == typeof exports ? module.exports = e(
		require("jquery")) : e(jQuery)
}(function(e) {
	function t(e) {
		return d.raw ? e : encodeURIComponent(e)
	}

	function n(e) {
		return d.raw ? e : decodeURIComponent(e)
	}

	function o(e) {
		return t(d.json ? JSON.stringify(e) : String(e))
	}

	function a(e) {
		0 === e.indexOf('"') && (e = e.slice(1, -1).replace(/\\"/g, '"').replace(/\\\\/g, "\\"));
		try {
			return e = decodeURIComponent(e.replace(i, " ")), d.json ? JSON.parse(e) : e
		} catch(t) {}
	}

	function p(i, t) {
		var n = d.raw ? i : a(i);
		return e.isFunction(t) ? t(n) : n
	}
	var i = /\+/g,
		d = e.cookie = function(i, a, r) {
			if (arguments.length > 1 && !e.isFunction(a)) {
				if (r = e.extend({}, d.defaults, r), "number" == typeof r.expires) {
					var s = r.expires,
						f = r.expires = new Date;
					f.setMilliseconds(f.getMilliseconds() + 864e5 * s)
				}
				return document.cookie = [t(i), "=", o(a), r.expires ? "; expires=" + r.expires.toUTCString() : "", r.path ?
					"; path=" + r.path : "", r.domain ? "; domain=" + r.domain : "", r.secure ? "; secure" : ""
				].join("")
			}
			for (var u = i ? void 0 : {}, l = document.cookie ? document.cookie.split("; ") : [], c = 0, g = l.length; g > c; c++) {
				var m = l[c].split("="),
					h = n(m.shift()),
					w = m.join("=");
				if (i === h) {
					u = p(w, a);
					break
				}
				i || void 0 === (w = p(w)) || (u[h] = w)
			}
			return u
		};
	d.defaults = {}, e.removeCookie = function(i, t) {
		return e.cookie(i, "", e.extend({}, t, {
			expires: -1
		})), !e.cookie(i)
	}
});
