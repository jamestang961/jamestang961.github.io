// 设置相关

var thePage;
$(document).on("app.ready", (function(e) {
	setAppDebugLogInit(), 
	appconfig.langprefix = appconfig.lang.substring(0, 1),
	appconfig.apptitle = "", 
	document.title = appconfig.apptitle, 
	setCurPage($("body")), 
	thePage = getCurPage(), 
	appconfig.HealthDeclarationConfig.title = lng("歡迎使用<br>【個人健康聲明】", 
	"Bem-vindo ao Sistema electrónico para Declaração de Saúde",
	"Welcome to the electronic system for “Personal Health Declaration”"), 
	appconfig.HealthDeclarationConfig.theme.default = appconfig.HealthDeclarationConfig.theme.phdform, 
	$("body").addClass(appconfig.HealthDeclarationConfig.theme.default.styleClass), 
	setHealthDeclarationConfig(), 
	PhdForm.showMain(), 
	removeAllSpinner(), 
	setTimeout((function() {
			setParentIframeHeight($("body"))
		}), 500)
}));
