// 通用api

var CommonApi = {};
CommonApi.Sms = {
	sendVerifyCode: function(e, t, n) {//发送验证码
		return webcall(APIURL, "SendVerifyCode", e, t, n)
	},
	checkVerifyCode: function(e, t, n) {//校验验证码
		return webcall(APIURL, "CheckVerifyCode", e, t, n)
	}
}, 
CommonApi.Captcha = {
	getData: function(e, t, n) {//获取验证日期
		return webcall(APIURL, "getCaptchaData", e, t, n)
	}
}, 
CommonApi.Export = {
	toCsv: function(e, t, n) {//导出Csv
		return webcall(APIURL, "exportCsv", e, t, n)
	}
}, 
CommonApi.App = {
	getAppSetting: function(e, t, n) {//获取应用程序设置
		return webcall(APIURL, "getAppSetting", e, t, n)
	}
};
